	<div id="map" style="height: 45rem;">
			<script>
				let lat = 19.484237817889223
				let lon = -99.12243962287903
				var map = L.map('map').setView([lat, lon], 18);
				L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
					maxZoom: 19,
					attribution: '&copy; <a href="http://www.openstreetmap.org/copyright">OpenStreetMap</a>'
				}).addTo(map);
				var marker = L.marker([lat, lon]).addTo(map);
				var popup = L.popup()
					.setLatLng([lat, lon])
					.setContent("AQUA JAKNET - Habana #, Tepeyac Insurgentes, Gustavo A. Madero, 07020 Ciudad de México, CDMX")
					.openOn(map);
			</script>
